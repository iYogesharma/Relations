    "keywords": [
        "iYogesharma",
        "relations",
        "custom",
        "belongsToManyThrough"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Yogesh Sharma",
        "email": "iyogesharma@gmail.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.*|6.*|7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0|~3.9.0"
    },
    "autoload": {
        "psr-4": {
            "YS\\Relations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YS\\Relations\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
